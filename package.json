{
  "name": "cloudflareRedirect",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --env dev",
    "build": "tsc -p .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "deploy:dev": "wrangler deploy --env dev",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:prod": "wrangler deploy --env production",
    "deploy:dry": "wrangler deploy --dry-run --env production",
    "logs:dev": "wrangler tail --env dev",
    "logs:staging": "wrangler tail --env staging",
    "logs:prod": "wrangler tail --env production",
    "kv:list": "wrangler kv:namespace list --env production",
    "kv:list:staging": "wrangler kv:namespace list --env staging",
    "secret:dev": "wrangler secret put GA4_API_SECRET --env dev",
    "secret:staging": "wrangler secret put GA4_API_SECRET --env staging",
    "secret:prod": "wrangler secret put GA4_API_SECRET --env production",
    "validate": "wrangler validate",
    "whoami": "wrangler whoami"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241004.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.1",
    "jsdom": "^27.0.1",
    "miniflare": "^3.20250718.2",
    "typescript": "^5.9.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.3",
    "wrangler": "^4.45.3"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.0",
    "hono": "^4.4.0",
    "zod": "^4.1.0"
  }
}